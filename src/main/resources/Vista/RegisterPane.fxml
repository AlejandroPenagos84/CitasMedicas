<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="302.0" style="-fx-background-color: #ffffff;" stylesheets="@../CSS/login.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controlador.RegisterController">
    <children>
        <ImageView fitHeight="62.0" fitWidth="77.0" layoutX="113.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/users-solid.jpg" />
            </image>
        </ImageView>
        <TextField layoutX="76.0" layoutY="128.0" promptText="Nombres" style="-fx-background-color: transparent; -fx-border-color: #57cac3; -fx-border-width: 0px 0px 2px 0px;" />
        <PasswordField layoutX="76.0" layoutY="207.0" promptText="Contraseña" style="-fx-background-color: transparent; -fx-border-color: #57cac3; -fx-border-width: 0px 0px 2px 0px;" />
        <TextField layoutX="77.0" layoutY="166.0" promptText="Apellidos" style="-fx-background-color: transparent; -fx-border-color: #57cac3; -fx-border-width: 0px 0px 2px 0px;" />
        <Button fx:id="loginOption" layoutX="103.0" layoutY="275.0" mnemonicParsing="false" onAction="#handleLoginOption" text="Iniciar Sesión" />
        <Button fx:id="registerButton" layoutX="103.0" layoutY="310.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="78.0" text="Registrarse" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
